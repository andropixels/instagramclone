<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.Homefragment">


    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/appbar_home_fragment"
        >


        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_home_fragment"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="@android:color/white"
            >
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/instaicon"
                    android:layout_width="60dp"
                    android:layout_height="match_parent"
                    android:src="@drawable/insta"
                    android:layout_alignParentLeft="true"

                    />
                <ImageView
                    android:id="@+id/sendicon"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_send"
                    android:layout_toEndOf="@+id/instaicon"
                   android:layout_alignParentRight="true"
                    android:padding="5dp"
                    android:layout_marginLeft="180dp"
                    />
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.recyclerview.widget.RecyclerView

        android:id="@+id/recyclerviewhomefragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/appbar_home_fragment"
        android:layout_margin="10dp"
        />



</RelativeLayout>